[core]
	repositoryformatversion = 0
	filemode = true
	bare = false
	logallrefupdates = true
[remote "origin"]
	fetch = +refs/heads/*:refs/remotes/origin/*
	url = https://username@github.com/username/project.git
[branch "master"]
	remote = origin
	merge = refs/heads/master

[alias]
	co = checkout
	ci = commit
	st = status



[comment]
	first commit with, `git push -u origin master`
	this file save at ~/project/.git/config


